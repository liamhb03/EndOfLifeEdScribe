<script>
var webflowAuth = {
  loginPath: '/log-in',
  loginRedirectPath: '/video',
  signupPath: '/sign-up',
  signupRedirectPath: '/video',
  logoutRedirectPath: '/',
  firebaseConfig: {
    apiKey: "AIzaSyC2pTE58Wf5pp66FdKcX6nbyGeVv2ShLlY",
    authDomain: "edscribe-fa4f5.firebaseapp.com",
    projectId: "edscribe-fa4f5",
    storageBucket: "edscribe-fa4f5.appspot.com",
    messagingSenderId: "478160457052",
    appId: "1:478160457052:web:d56d9edaf5b35b45304599",
    measurementId: "G-7KJXCE6FDB"
  }
};

/*
Example config
